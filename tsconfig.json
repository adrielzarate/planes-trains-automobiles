{
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": "src",
        "sourceMap": true,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "allowJs": true,
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "target": "es5",
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}